# Gdscript rules


Comments = [[##, ]]

Text = [[", \", /", "], [', \', /', ']]


Dependencies {
    Extends {
        # class ChildClass extends ParentClass
        pattern: "^[\t ]*extends\s+(\w+)"
        total_number: 1
    }
}


Identifiers {
    Class name {
        # class_name ClassName
        pattern: "^[\t ]*class_name\s+(\w+)"
        total_number: 1
    }
}
